			"  check_timeout = \"${76:<check-timeout>}\"",
			"",
			"  // consul disable_registration",
			"  disable_registration = ${77|true,false|}",
			"",
			"  // consul scheme",
			"  scheme = \"${78:<scheme>}\" # (http|https)",
			"",
			"  // consul service",
			"  // specifies the name of the service to register in Consul.",
			"  service         = \"${79:<service>}\"",
			"  // consul service_tags",
