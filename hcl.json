	},
	"consul-helm-chart configuration 'global' - full": {
		"prefix": ["consul-helm-chart-configuration-global-full"],
		"body": [
			"# holds values that affect multiple components of the chart.",
			"global:",
			"  # enabled",
			"  # the main enabled/disabled setting.",
			"  # if true, servers, clients, Consul DNS and the Consul UI will be enabled.",
			"  # each component can override this default via its component-specific \"enabled\" config.",
			"  # if false, no components will be installed by default and per-component opt-in is required, such as by setting 'server.enabled' to true.",
			"  enabled: ${1|true,false|}",
			"",
			"  # logLevel",
			"  # the default log level to apply to all components which do not otherwise override this setting.",
			"  # it is recommended to generally not set this below \"info\" unless actively debugging due to logging verbosity.",
			"  # one of \"debug\", \"info\", \"warn\", or \"error\".",
			"  logLevel: ${2|error,warn,info,debug|}",
			"",
			"  # logJSON",
			"  # enable all component logs to be output in JSON format.",
			"  logJSON: ${3|true,false|}",
			"",
			"  # name",
			"  # set the prefix used for all resources in the Helm chart.",
			"  # if not set, the prefix will be '<helm release name>-consul'.",
			"  name: ${3:<resources-prefix>}",
			"",
			"  # domain",
			"  # the domain Consul will answer DNS queries for (see '-domain' [https://consul.io/docs/agent/options#_domain]) and the domain services synced from Consul into Kubernetes will have,",
			"  # e.g. 'service-name.service.consul'.",
			"  domain: ${4:<domain-name>}",
			"",
			"  # ! enterprise feature !",
			"  # enabling adminPartitions allows creation of Admin Partitions in Kubernetes clusters.",
			"  # it additionally indicates that you are running Consul Enterprise v1.11+ with a valid Consul Enterprise license.",
			"  # admin partitions enables deploying services across partitions, while sharing a set of Consul servers.",
			"  adminPartitions:",
			"    # if true, the Helm chart will enable Admin Partitions for the cluster.",
			"    # the clients in the server cluster must be installed in the default partition.",
			"    # creation of Admin Partitions is only supported during installation.",
			"    # Admin Partitions cannot be installed via a Helm upgrade operation.",
			"    # only Helm installs are supported.",
			"    # defaults to false.",
			"    enabled: ${5|true,false|}",
			"",
			"    # the name of the Admin Partition.",
			"    # the partition name cannot be modified once the partition has been installed.",
			"    # changing the partition name would require an un-install and a re-install with the updated name.",
			"    # must be \"default\" in the server cluster ie the Kubernetes cluster that the Consul server pods are deployed onto.",
			"    # default to \"default\".",
			"    name: \"${6:<admin-partition-name>}\"",
			"",
			"    # partition service properties.",
			"    service:",
			"      type: ${7:LoadBalancer}",
			"",
			"      # optionally set the nodePort value of the partition service if using a NodePort service.",
      "      # if not set and using a NodePort service, Kubernetes will automatically assign a port.",
			"      nodePort:",
			"        # rpc",
			"        rpc: ${8:<rpc-node-port>}",
			"",
			"        # serf",
			"        serf: ${9:<serf-node-port>}",
			"",
			"        # https",
			"        https: ${10:<https-node-port>}",
			"",
			"      # annotations to apply to the partition service.",
			"      # ",
			"      # ```yaml",
			"      # annotations: |",
			"      #   \"annotation-key\": \"annotation-value\"",
			"      # ```",
			"      annotations: \"${11:<annotations-string>}\"",
			"",
			"  # image",
			"  # the name (and tag) of the Consul Docker image for clients and servers.",
			"  # this can be overridden per component. This should be pinned to a specific version tag, otherwise you may inadvertently upgrade your Consul version.",
			"  # examples:",
			"  #   image: \"consul:1.10.0\"",
			"  #   image: \"hashicorp/consul-enterprise:1.10.0-ent\"",
			"  # defaults to \"hashicorp/consul:<latest version>\"",
			"  image: ${12:<image-name-with-tag>}",
			"",
			"  # imagePullSecrets",
			"  # array of objects containing image pull secret names that will be applied to each service account.",
			"  # this can be used to reference image pull secrets if using a custom consul or consul-k8s-control-plane Docker image.",
			"  # see https://kubernetes.io/docs/concepts/containers/images/#using-a-private-registry for reference.",
			"  imagePullSecrets:",
			"    - ${13:<pull-secret-name1>}",
			"    - ${14:<pull-secret-name2>}",
			"    - ${15:<pull-secret-nameN>}",
			"",
			"  # imageK8S",
			"  # the name (and tag) of the consul-k8s-control-plane Docker image that is used for functionality such as catalog sync.",
			"  # this can be overridden per component.",
			"  # defaults to \"hashicorp/consul-k8s-control-plane:<latest version>\".",
			"  imageK8S: ${16:<consul-k8s-control-plane-image-name-and-tag>}",
			"",
			"  # datacenter",
			"  # the name of the datacenter that the agents should register as.",
			"  # this can't be changed once the Consul cluster is up and running since Consul doesn't support an automatic way to change this value currently.",
			"  # [https://github.com/hashicorp/consul/issues/1858]",
			"  datacenter: ${17:<datacenter-name>}",
			"",
			"  # enablePodSecurityPolicies",
			"  # controls whether pod security policies are created for the Consul components created by this chart.",
			"  # see https://kubernetes.io/docs/concepts/policy/pod-security-policy/.",
			"  # defaults to false.",
			"  enablePodSecurityPolicies: ${18|true,false|}",
			"",
			"  # secretsBackend",
			"  # secretsBackend is used to configure Vault as the secrets backend for the Consul on Kubernetes installation.",
			"  # the Vault cluster needs to have the Kubernetes Auth Method, KV2 and PKI secrets engines enabled",
			"  # and have necessary secrets, policies and roles created prior to installing Consul.",
			"  # see https://www.consul.io/docs/k8s/installation/vault for full instructions.",
			"  #",
			"  # the Vault cluster _must_ not have the Consul cluster installed by this Helm chart as its storage backend",
			"  # as that would cause a circular dependency.",
			"  # Vault can have Consul as its storage backend as long as that Consul cluster is not running on this Kubernetes cluster",
			"  # and is being managed separately from this Helm installation.",
			"  #",
			"  # Note: When using Vault KV2 secrets engines the \"data\" field is implicitly required for Vault API calls,",
			"  # secretName should be in the form of  \"vault-kv2-mount-path/data/secret-name\".",
			"  # secretKey should be in the form of \"key\".",
			"  secretsBackend:",
			"    # vault",
			"    vault:",
			"      # enabled",
			"      # enabling the Vault secrets backend will replace Kubernetes secrets with referenced Vault secrets.",
			"      enabled: ${19|true,false|}",
			"",
			"      # consulServerRole",
			"      # the Vault role for the Consul server.",
      "      # the role must be connected to the Consul server's service account and",
      "      # have a policy with read capabilities for the following secrets:",
      "      #   - gossip encryption key defined by 'global.gossipEncryption.secretName'",
      "      #   - certificate issue path defined by 'server.serverCert.secretName'",
      "      #   - CA certificate defined by 'global.tls.caCert.secretName'",
      "      #   - replication token defined by 'global.acls.replicationToken.secretName' if 'global.federation.enabled' is 'true'",
      "      # to discover the service account name of the Consul server, run",
      "      #     ```shell-session",
      "      #     $ helm template --show-only templates/server-serviceaccount.yaml <release-name> hashicorp/consul",
      "      #     ```",
      "      # and check the name of 'metadata.name'.",
			"      # defaults to \"\".",
			"      consulServerRole: \"${20:<consul-server-vault-role>}\"",
			"",
			"      # consulClientRole",
			"      # the Vault role for the Consul client.",
      "      # the role must be connected to the Consul client's service account and",
      "      # have a policy with read capabilities for the following secrets:",
      "      #   - gossip encryption key defined by 'global.gossipEncryption.secretName'.",
      "      # to discover the service account name of the Consul client, run",
      "      #     ```shell-session",
      "      #     $ helm template --show-only templates/client-serviceaccount.yaml <release-name> charts/consul",
      "      #     ```",
      "      # and check the name of 'metadata.name'.",
			"      # defaults to \"\".",
			"      consulClientRole: \"${21:<consul-client-vault-role>}\"",
			"",
			"      # manageSystemACLsRole",
			"      # a Vault role to allow Kubernetes job that manages ACLs for this Helm chart ('server-acl-init')",
      "      # to read and update Vault secrets for the Consul's bootstrap and replication tokens.",
      "      # this role must be bound the 'server-acl-init's service account.",
      "      # to discover the service account name of the 'server-acl-init' job, run",
      "      #     ```shell-session",
      "      #     $ helm template --show-only templates/server-acl-init-serviceaccount.yaml <release-name> charts/consul",
      "      #     ```",
      "      # and check the name of 'metadata.name'.",
			"      manageSystemACLsRole: \"${22:<system-acls-management-role>}\"",
			"",
			"      # agentAnnotations",
			"      # this value defines additional annotations for Vault agent on any pods where it'll be running.",
      "      # this should be formatted as a multi-line string.",
      "      #   ```yaml",
      "      #   annotations: |",
      "      #     \"sample/annotation1\": \"foo\"",
      "      #     \"sample/annotation2\": \"bar\"",
      "      #   ```",
			"      agentAnnotations: |",
			"        \"${23:<agent-annotation-key1>}\": \"${24:<agent-annotation-value1>}\"",
			"        \"${25:<agent-annotation-key2>}\": \"${26:<agent-annotation-value2>}\"",
			"        \"${27:<agent-annotation-keyN>}\": \"${28:<agent-annotation-valueN>}\"",
			"",
			"      # ca",
			"      ca:",
			"        # secretName",
			"        # secretName is the name of the Kubernetes secret that holds the Vault CA certificate.",
			"        # a Kubernetes secret must be in the same namespace that Consul is installed into.",
			"        secretName: \"${11:<kubernetes-secret-name-for-vault-ca-certificate>}\"",
			"        # secretKey",
			"        # secretKey is the key within the Kubernetes secret that holds the Vault CA certificate.",
			"        secretKey: \"${29:<kubernetes-secret-key-for-vault-ca-certificate>}\"",
			"",
			"      # connectCA",
			"      # configuration for the Vault Connect CA provider.",
      "      # the provider will be configured to use the Vault Kubernetes auth method",
      "      # and therefore requires the role provided by 'global.secretsBackend.vault.consulServerRole'",
      "      # to have permissions to the root and intermediate PKI paths.",
      "      # please see https://www.consul.io/docs/connect/ca/vault#vault-acl-policies",
      "      # for information on how to configure the Vault policies.",
			"      connectCA:",
			"        # address",
			"        # the address of the Vault server.",
			"        address: \"${30:<vault-server-address>}\"",
			"",
			"        # authMethodPath",
			"        # the mount path of the Kubernetes auth method in Vault.",
			"        authMethodPath: \"${31:<vault-mount-path-of-k8s-auth-method>}\"",
			"",
			"        # rootPKIPath",
			"        # the path to a PKI secrets engine for the root certificate.",
			"        # please see https://www.consul.io/docs/connect/ca/vault#rootpkipath.",
			"        rootPKIPath: \"${32:<vault-path-of-pki-secrets-engine-for-root-certificate>}\"",
			"",
			"        # intermediatePKIPath",
			"        # the path to a PKI secrets engine for the generated intermediate certificate.",
			"        # please see https://www.consul.io/docs/connect/ca/vault#intermediatepkipath.",
			"        intermediatePKIPath: \"${33:<vault-path-of-pki-secrets-engine-for-intermediate-certificate>}\"",
			"",
			"        # additionalConfig",
			"        # additional Connect CA configuration in JSON format.",
			"        # please see https://www.consul.io/docs/connect/ca/vault#common-ca-config-options for additional configuration options.",
			"        # example:",
			"        #   ```yaml",
			"        #   additionalConfig: |",
			"        #     {",
			"        #       \"connect\": [{",
			"        #         \"ca_config\": [{",
			"        #              \"leaf_cert_ttl\": \"36h\"",
			"        #           }]",
			"        #       }]",
			"        #     }",
			"        #   ```",
			"        additionalConfig: |",
			"          {",
			"            \"${34:<insert-additional-configuration-here>}\"",
			"          }",
			"",
			"  # gossipEncryption",
			"  gossipEncryption:",
			"    # autoGenerate",
			"    # automatically generate a gossip encryption key and save it to a Kubernetes secret.",
			"    autoGenerate: ${35|true,false|}",
			"",
			"    # secretName",
			"    # secretName is the name of the Kubernetes secret or Vault secret path that holds the gossip encryption key.",
			"    # a Kubernetes secret must be in the same namespace that Consul is installed into.",
			"    secretName: \"${36:<name-of-k8s-or-vault-secret-path-that-holds-the-gossip-encryption-key>}\"",
			"",
			"    # secretKey",
			"    # secretKey is the key within the Kubernetes secret or Vault secret key that holds the gossip encryption key.",
			"    secretKey: \"${37:<name-of-k8s-or-vault-secret-key-that-holds-the-gossip-encryption-key>}\"",
			"",
			"  # recursors",
			"  # a list of addresses of upstream dns servers that are used to recursively resolve dns queries.",
			"  # these values are given as '-recursor' flags to Consul servers and clients.",
			"  # see https://www.consul.io/docs/agent/options#_recursor for more details.",
			"  # if this is an empty array (the default), then Consul dns will only resolve queries for the Consul top level domain (by default '.consul').",
			"  recursors:",
			"   - \"${38:<upstream-dns-server1>}\"",
			"   - \"${39:<upstream-dns-server2>}\"",
			"   - \"${40:<upstream-dns-serverN>}\"",
			"",
			"  # tls",
			"  # enables tls (https://learn.hashicorp.com/tutorials/consul/tls-encryption-secure)",
			"  # across the cluster to verify authenticity of the Consul servers and clients.",
			"  # requires Consul v1.4.1+.",
			"  tls:",
			"    # enabled",
			"    # if true, the Helm chart will enable tls for Consul servers and clients and all consul-k8s-control-plane components,",
			"    # as well as generate certificate authority (optional) and server and client certificates.",
			"    enabled: ${41|true,false|}",
			"",
			"    # enableAutoEncrypt",
			"    # if true, turns on the auto-encrypt feature on clients and servers.",
			"    # it also switches consul-k8s-control-plane components to retrieve the CA from the server via the API.",
			"    # requires Consul 1.7.1+.",
			"    enableAutoEncrypt: ${42|true,false|}",
			"",
			"    # serverAdditionalDNSSANs",
			"    # a list of additional DNS names to set as Subject Alternative Names (SANs) in the server certificate.",
			"    # this is useful when you need to access the Consul server(s) externally, for example, if you're using the ui.",
			"    serverAdditionalDNSSANs:",
			"      - \"${43:<subject-alternative-name1>}\"",
			"      - \"${44:<subject-alternative-name2>}\"",
			"      - \"${45:<subject-alternative-nameN>}\"",
			"",
			"    # serverAdditionalIPSANs",
			"    # a list of additional ip addresses to set as Subject Alternative Names (SANs) in the server certificate.",
			"    # this is useful when you need to access the Consul server(s) externally, for example, if you're using the ui.",
			"    serverAdditionalIPSANs:",
			"      - \"${46:<subject-alternative-ip1>}\"",
			"      - \"${47:<subject-alternative-ip2>}\"",
			"      - \"${48:<subject-alternative-ipN>}\"",
			"",
			"    # verify",
			"    # if true, 'verify_outgoing', 'verify_server_hostname' and 'verify_incoming_rpc' will be set to 'true' for Consul servers and clients.",
			"    # set this to false to incrementally roll out tls on an existing Consul cluster.",
			"    # please see https://consul.io/docs/k8s/operations/tls-on-existing-cluster for more details.",
			"    verify: ${49|true,false|}",
			"",
			"    # httpsOnly",
			"    # if true, the Helm chart will configure Consul to disable the HTTP port on both clients and servers and to only accept https connections.",
			"    httpsOnly: ${50|true,false|}",
			"",
			"    # caCert",
			"    # a secret containing the certificate of the CA to use for tls communication within the Consul cluster.",
			"    # if you have generated the CA yourself with the Consul cli, you could use the following command to create the secret in Kubernetes:",
			"    #   ```shell-session",
			"    #   $ kubectl create secret generic consul-ca-cert --from-file='tls.crt=./consul-agent-ca.pem'",
			"    #   ```",
			"    # if you are using Vault as a secrets backend with tls, 'caCert.secretName' must be provided and should reference the CA path for your PKI secrets engine.",
			"    # this should be of the form 'pki/cert/ca' where 'pki' is the mount point of your pki secrets engine.",
			"    # a read policy must be created and associated with the CA cert path for 'global.tls.caCert.secretName'.",
			"    # this will be consumed by the 'global.secretsBackend.vault.consulCARole' role by all Consul components.",
			"    # when using Vault the secretKey is not used.",
			"    caCert:",
			"      # secretName",
			"      # the name of the Kubernetes secret.",
			"      secretName: ${51:\"<kubernetes-secret-name>\"}",
			"",
			"      # secretKey",
			"      # the key of the Kubernetes secret.",
			"      secretKey: ${52:\"<kubernetes-secret-key>\"}",
			"",
			"  # enableConsulNamespaces",
			"  # ! enterprise feature !",
			"  # 'enableConsulNamespaces' indicates that you are running Consul Enterprise v1.7+ with a valid Consul Enterprise license and would",
			"  # like to make use of configuration beyond registering everything into the 'default' Consul namespace.",
			"  # additional configuration options are found in the 'consulNamespaces' section of both the catalog sync and connect injector.",
			"  enableConsulNamespaces: ${53|true,false|}",
			"",
			"  # acls",
			"  acls:",
			"    # manageSystemACLs",
			"    # if true, the Helm chart will automatically manage ACL tokens and policies for all Consul and consul-k8s-control-plane components.",
			"    # this requires Consul >= 1.4.",
			"    manageSystemACLs: ${54|true,false|}",
			"",
			"    # bootstrapToken",
			"    # a Kubernetes secret containing the bootstrap token to use for creating policies and tokens for all Consul and consul-k8s-control-plane components.",
			"    # if set, we will skip acl bootstrapping of the servers and will only initialize acls for the Consul clients and consul-k8s-control-plane system components.",
			"    bootstrapToken:",
			"      # secretName",
			"      # the name of the Kubernetes secret.",
			"      secretName: ${55:\"<kubernetes-secret-name>\"}",
			"",
			"      # secretKey",
			"      # the key of the Kubernetes secret.",
			"      secretKey: ${56:\"<kubernetes-secret-key>\"}",
			"",
			"    # createReplicationToken",
			"    # if true, an acl token will be created that can be used in secondary datacenters for replication.",
			"    # this should only be set to true in the primary datacenter since the replication token must be created from that datacenter.",
			"    # in secondary datacenters, the secret needs to be imported from the primary datacenter and referenced via 'global.acls.replicationToken'.",
			"    createReplicationToken: ${56|true,false|}",
			"",
			"    # replicationToken",
			"    # replicationToken references a secret containing the replication ACL token.",
			"    # this token will be used by secondary datacenters to perform ACL replication and create ACL tokens and policies.",
			"    # this value is ignored if 'bootstrapToken' is also set.",
			"    replicationToken:",
			"      # secretName",
			"      # the name of the Kubernetes secret or the path of the secret in Vault.",
			"      secretName: ${58:\"<name-of-k8s-secret-or-vault-secret-path>\"}",
			"",
			"      # secretKey",
			"      # the key of the Kubernetes or Vault secret.",
			"      secretKey: ${59:\"<k8s-or-vault-secret-key>\"}",
			"",
			"    # partitionToken",
			"    # partitionToken references a Vault secret containing the acl token to be used in non-default partitions.",
			"    # this value should only be provided in the default partition and only when setting  'global.secretsBackend.vault.enabled' to 'true'.",
			"    # we will use the value of the secret stored in Vault to create an acl token in Consul with the value of the secret as the 'secretID' for the token.",
			"    # in non-default, partitions set this secret as the 'bootstrapToken'.",
			"    partitionToken:",
			"      # secretName",
			"      # the name of the Kubernetes secret or the path of the secret in Vault.",
			"      secretName: ${60:\"<name-of-vault-secret-path>\"}",
			"",
			"      # secretKey",
			"      # the key of the Kubernetes or Vault secret.",
			"      secretKey: ${61:\"<vault-secret-key>\"}",
			"",
			"  # enterpriseLicense",
			"  # ! enterprise license !",
			"  # this value refers to a Kubernetes secret that you have created that contains your enterprise license.",
			"  # it is required if you are using an enterprise binary.",
			"  # defining it here applies it to your cluster once a leader has been elected.",
			"  # if you are not using an enterprise image or if you plan to introduce the license key via another route, then set these fields to null.",
			"  # note: the job to apply license runs on both Helm installs and upgrades.",
			"  enterpriseLicense:",
			"    # secretName",
			"    # secretName is the name of the Kubernetes secret or Vault secret path that holds the enterprise license.",
			"    # a Kubernetes secret must be in the same namespace that Consul is installed into.",
			"    secretName: ${62:\"<name-of-k8s-secret-or-vault-secret-path>\"}",
			"",
			"    # secretKey",
			"    # secretKey is the key within the Kubernetes secret or Vault secret key that holds the enterprise license.",
			"    secretKey: ${63:\"<k8s-or-vault-secret-key>\"}",
			"",
			"    # enableLicenseAutoload",
			"    # manages license autoload.",
			"    # required in Consul 1.10.0+, 1.9.7+ and 1.8.12+.",
			"    enableLicenseAutoload: ${64|true,false|}",
			"",
			"  # federation",
			"  federation:",
			"    # enabled",
			"    # if enabled, this datacenter will be federation-capable.",
			"    # only federation via mesh gateways is supported.",
			"    # mesh gateways and servers will be configured to allow federation.",
			"    # requires 'global.tls.enabled', 'meshGateway.enabled' and 'connectInject.enabled' to be true.",
			"    # requires Consul 1.8+.",
			"    enabled: ${65|true,false|}",
			"",
			"    # createFederationSecret",
			"    # if true, the chart will create a Kubernetes secret that can be imported into secondary datacenters so they can federate with this datacenter.",
			"    # the secret contains all the information secondary datacenters need to contact and authenticate with this datacenter.",
			"    # this should only be set to true in your primary datacenter.",
			"    # the secret name is '<global.name>-federation' (if setting 'global.name'), otherwise '<helm-release-name>-consul-federation'.",
			"    createFederationSecret: ${66|true,false|}",
			"",
			"    # primaryDatacenter",
			"    # the name of the primary datacenter.",
			"    primaryDatacenter: \"${67:<primary-datacenter-name>}\"",
			"",
			"    # primaryGateways",
			"    # a list of addresses of the primary mesh gateways in the form '<ip>:<port>'.",
			"    # (e.g. [\"1.1.1.1:443\", \"2.3.4.5:443\"]",
			"    primaryGateways: [",
			"      \"${68:<gateway1-ip>}:${69:<gateway1-port>}\",",
			"      \"${70:<gateway2-ip>}:${71:<gateway2-port>}\",",
			"      \"${72:<gatewayN-ip>}:${73:<gatewayN-port>}\"",
			"    ]",
			"",
			"    # k8sAuthMethodHost",
			"    # if you are setting 'global.federation.enabled' to 'true' and are in a secondary datacenter,",
			"    # set 'k8sAuthMethodHost' to the address of the Kubernetes API server of the secondary datacenter.",
			"    # this address must be reachable from the Consul servers in the primary datacenter.",
			"    # this authmethod will be used to provision acl tokens for Consul components and is different from the one used by the Consul Service Mesh.",
			"    # please see the Kubernetes Auth Method documentation (https://consul.io/docs/acl/auth-methods/kubernetes).",
			"    # you could retrieve this value from your 'kubeconfig' by running:",
			"    #   ```shell-session",
			"    #   $ kubectl config view -o jsonpath=\"{.clusters[?(@.name=='<your cluster name>')].cluster.server}\"",
			"    #   ```",
			"    k8sAuthMethodHost: \"${74:<kubernetes-api-server>}\"",
			"",
			"  # metrics",
			"  metrics:",
			"    # enabled",
			"    # configures the Helm chart’s components to expose Prometheus metrics for the Consul service mesh.",
			"    # by default this includes gateway metrics and sidecar metrics.",
			"    enabled: ${75|true,false|}",
			"",
			"    # enableAgentMetrics",
			"    # configures consul agent metrics. Only applicable if 'global.metrics.enabled' is 'true'.",
			"    enableAgentMetrics: ${76|true,false|}",
			"",
			"    # agentMetricsRetentionTime",
			"    # configures the retention time for metrics in Consul clients and servers.",
			"    # this must be greater than 0 for Consul clients and servers to expose any metrics at all.",
			"    # only applicable if 'global.metrics.enabled' is true.",
			"    agentMetricsRetentionTime: \"${77:<retention-time>}\"",
			"",
			"    # enableGatewayMetrics",
			"    # if true, mesh, terminating, and ingress gateways will expose their Envoy metrics on port '20200' at the '/metrics' path and all gateway pods",
			"    # will have Prometheus scrape annotations.",
			"    # only applicable if 'global.metrics.enabled' is true.",
			"    enableGatewayMetrics: ${78|true,false|}",
			"",
			"  # consulSidecarContainer",
			"  # for connect-injected pods, the consul sidecar is responsible for metrics merging. For ingress/mesh/terminating",
			"  # gateways, it additionally ensures the Consul services are always registered with their local Consul client.",
			"  consulSidecarContainer:",
			"    # resources",
			"    # set default resources for consul sidecar.",
			"    # if null, that resource won't be set.",
			"    # these settings can be overridden on a per-pod basis via these annotations:",
			"    #   - 'consul.hashicorp.com/consul-sidecar-cpu-limit'",
			"    #   - 'consul.hashicorp.com/consul-sidecar-cpu-request'",
			"    #   - 'consul.hashicorp.com/consul-sidecar-memory-limit'",
			"    #   - 'consul.hashicorp.com/consul-sidecar-memory-request'",
			"    resources:",
			"      requests:",
			"        memory: \"${79:<requests-memory>}\"",
			"        cpu: \"${80:<requests-cpu>}\"",
			"      limits:",
			"        memory: \"${81:<limits-memory>}\"",
			"        cpu: \"${81:<limits-cpu>}\"",
			"",
			"  # imageEnvoy",
			"  # the name (and tag) of the Envoy Docker image used for the connect-injected sidecar proxies and mesh, terminating, and ingress gateways.",
			"  # see https://www.consul.io/docs/connect/proxies/envoy for full compatibility matrix between Consul and Envoy.",
			"  # defaults to \"envoyproxy/envoy-alpine:<latest supported version>\".",
			"  imageEnvoy: \"${82:<envoy-docker-container-image-name-and-tag>}\"",
			"",
			"  # openshift",
			"  # configuration for running this Helm chart on the Red Hat OpenShift platform.",
			"  # this Helm chart currently supports OpenShift v4.x+.",
			"  openshift:",
			"    # enabled",
			"    # if true, the Helm chart will create necessary configuration for running its components on OpenShift.",
			"    enabled: ${83|true,false|}",
			"",
			"$0"
		],
		"description": "consul-helm-chart full 'global' configuration"
	},
	"consul-helm-chart configuration 'global' - common": {
		"prefix": ["consul-helm-chart-configuration-global-common"],
		"body": [
			"# holds values that affect multiple components of the chart.",
			"global:",
			"  # enabled",
			"  # the main enabled/disabled setting.",
			"  # if true, servers, clients, Consul DNS and the Consul UI will be enabled.",
			"  # each component can override this default via its component-specific \"enabled\" config.",
			"  # if false, no components will be installed by default and per-component opt-in is required, such as by setting 'server.enabled' to true.",
			"  enabled: ${1|true,false|}",
			"",
			"  # logLevel",
			"  # the default log level to apply to all components which do not otherwise override this setting.",
			"  # it is recommended to generally not set this below \"info\" unless actively debugging due to logging verbosity.",
			"  # one of \"debug\", \"info\", \"warn\", or \"error\".",
			"  logLevel: ${2|error,warn,info,debug|}",
			"",
			"  # logJSON",
			"  # enable all component logs to be output in JSON format.",
			"  logJSON: ${3|true,false|}",
			"",
			"  # name",
			"  # set the prefix used for all resources in the Helm chart.",
			"  # if not set, the prefix will be '<helm release name>-consul'.",
			"  name: ${4:null}",
			"",
			"  # domain",
			"  # the domain Consul will answer DNS queries for (see '-domain' [https://consul.io/docs/agent/options#_domain]) and the domain services synced from Consul into Kubernetes will have,",
			"  # e.g. 'service-name.service.consul'.",
			"  domain: ${5:consul}",
			"",
			"  # ! enterprise feature !",
			"  # enabling adminPartitions allows creation of Admin Partitions in Kubernetes clusters.",
			"  # it additionally indicates that you are running Consul Enterprise v1.11+ with a valid Consul Enterprise license.",
			"  # admin partitions enables deploying services across partitions, while sharing a set of Consul servers.",
			"  adminPartitions:",
			"    # if true, the Helm chart will enable Admin Partitions for the cluster.",
			"    # the clients in the server cluster must be installed in the default partition.",
			"    # creation of Admin Partitions is only supported during installation.",
			"    # Admin Partitions cannot be installed via a Helm upgrade operation.",
			"    # only Helm installs are supported.",
			"    # defaults to false.",
			"    enabled: ${6:false}",
			"",
			"    # the name of the Admin Partition.",
			"    # the partition name cannot be modified once the partition has been installed.",
			"    # changing the partition name would require an un-install and a re-install with the updated name.",
			"    # must be \"default\" in the server cluster ie the Kubernetes cluster that the Consul server pods are deployed onto.",
			"    # default to \"default\".",
			"    name: \"${7:<admin-partition-name>}\"",
			"",
			"    # partition service properties.",
			"    service:",
			"      type: ${8:LoadBalancer}",
			"",
			"      # optionally set the nodePort value of the partition service if using a NodePort service.",
      "      # if not set and using a NodePort service, Kubernetes will automatically assign a port.",
			"      nodePort: ${9:{\\}}",
			"",
			"      # annotations to apply to the partition service.",
			"      #   ```yaml",
			"      #   annotations: |",
			"      #     \"annotation-key\": \"annotation-value\"",
			"      #   ```",
			"      annotations: ${10:null}",
			"",
			"  # image",
			"  # the name (and tag) of the Consul Docker image for clients and servers.",
			"  # this can be overridden per component. This should be pinned to a specific version tag, otherwise you may inadvertently upgrade your Consul version.",
			"  # examples:",
			"  #   image: \"consul:1.10.0\"",
			"  #   image: \"hashicorp/consul-enterprise:1.10.0-ent\"",
			"  # defaults to \"hashicorp/consul:<latest version>\"",
			"  image: ${11:<image-name-with-tag>}",
			"",
			"  # imagePullSecrets",
			"  # array of objects containing image pull secret names that will be applied to each service account.",
			"  # this can be used to reference image pull secrets if using a custom consul or consul-k8s-control-plane Docker image.",
			"  # see https://kubernetes.io/docs/concepts/containers/images/#using-a-private-registry for reference.",
			"  imagePullSecrets: ${12:[]}",
			"",
			"  # imageK8S",
			"  # the name (and tag) of the consul-k8s-control-plane Docker image that is used for functionality such as catalog sync.",
			"  # this can be overridden per component.",
			"  # defaults to \"hashicorp/consul-k8s-control-plane:<latest version>\".",
			"  imageK8S: ${13:<consul-k8s-control-plane-image-name-and-tag>}",
			"",
			"  # datacenter",
			"  # the name of the datacenter that the agents should register as.",
			"  # this can't be changed once the Consul cluster is up and running since Consul doesn't support an automatic way to change this value currently.",
			"  # [https://github.com/hashicorp/consul/issues/1858]",
			"  datacenter: ${14:<datacenter-name>}",
			"",
			"  # enablePodSecurityPolicies",
			"  # controls whether pod security policies are created for the Consul components created by this chart.",
			"  # see https://kubernetes.io/docs/concepts/policy/pod-security-policy/.",
			"  # defaults to false.",
			"  enablePodSecurityPolicies: ${15:false}",
			"",
			"  # secretsBackend",
			"  # secretsBackend is used to configure Vault as the secrets backend for the Consul on Kubernetes installation.",
			"  # the Vault cluster needs to have the Kubernetes Auth Method, KV2 and PKI secrets engines enabled",
			"  # and have necessary secrets, policies and roles created prior to installing Consul.",
			"  # see https://www.consul.io/docs/k8s/installation/vault for full instructions.",
			"  #",
			"  # the Vault cluster _must_ not have the Consul cluster installed by this Helm chart as its storage backend",
			"  # as that would cause a circular dependency.",
			"  # Vault can have Consul as its storage backend as long as that Consul cluster is not running on this Kubernetes cluster",
			"  # and is being managed separately from this Helm installation.",
			"  #",
			"  # Note: When using Vault KV2 secrets engines the \"data\" field is implicitly required for Vault API calls,",
			"  # secretName should be in the form of  \"vault-kv2-mount-path/data/secret-name\".",
			"  # secretKey should be in the form of \"key\".",
			"  secretsBackend:",
			"    # vault",
			"    vault:",
			"      # enabled",
			"      # enabling the Vault secrets backend will replace Kubernetes secrets with referenced Vault secrets.",
			"      enabled: ${16|true,false|}",
			"",
			"      # consulServerRole",
			"      # the Vault role for the Consul server.",
      "      # the role must be connected to the Consul server's service account and",
      "      # have a policy with read capabilities for the following secrets:",
      "      #   - gossip encryption key defined by 'global.gossipEncryption.secretName'",
      "      #   - certificate issue path defined by 'server.serverCert.secretName'",
      "      #   - CA certificate defined by 'global.tls.caCert.secretName'",
      "      #   - replication token defined by 'global.acls.replicationToken.secretName' if 'global.federation.enabled' is 'true'",
      "      # to discover the service account name of the Consul server, run",
      "      #     ```shell-session",
      "      #     $ helm template --show-only templates/server-serviceaccount.yaml <release-name> hashicorp/consul",
      "      #     ```",
      "      # and check the name of 'metadata.name'.",
			"      # defaults to \"\".",
			"      consulServerRole: \"${17:<consul-server-vault-role>}\"",
			"",
			"      # consulClientRole",
			"      # the Vault role for the Consul client.",
      "      # the role must be connected to the Consul client's service account and",
      "      # have a policy with read capabilities for the following secrets:",
      "      #   - gossip encryption key defined by 'global.gossipEncryption.secretName'.",
      "      # to discover the service account name of the Consul client, run",
      "      #     ```shell-session",
      "      #     $ helm template --show-only templates/client-serviceaccount.yaml <release-name> charts/consul",
      "      #     ```",
      "      # and check the name of 'metadata.name'.",
			"      # defaults to \"\".",
			"      consulClientRole: \"${18:<consul-client-vault-role>}\"",
			"",
			"      # manageSystemACLsRole",
			"      # a Vault role to allow Kubernetes job that manages ACLs for this Helm chart ('server-acl-init')",
      "      # to read and update Vault secrets for the Consul's bootstrap and replication tokens.",
      "      # this role must be bound the 'server-acl-init's service account.",
      "      # to discover the service account name of the 'server-acl-init' job, run",
      "      #     ```shell-session",
      "      #     $ helm template --show-only templates/server-acl-init-serviceaccount.yaml <release-name> charts/consul",
      "      #     ```",
      "      # and check the name of 'metadata.name'.",
			"      manageSystemACLsRole: \"${4:<system-acls-management-role>}\"",
			"",
			"      # consulCARole",
			"      # the Vault role for all Consul components to read the Consul's server's CA Certificate (unauthenticated).",
      "      # the role should be connected to the service accounts of all Consul components, or alternatively '*' since it",
      "      # will be used only against the 'pki/cert/ca' endpoint which is unauthenticated.",
			"      # a policy must be created which grants read capabilities to 'global.tls.caCert.secretName', which is usually 'pki/cert/ca'.",
			"      # default to \"\".",
			"      consulCARole: \"${19:<consul-ca-certificate-vault-role>}\"",
			"",
			"      # ca",
			"      ca:",
			"        # secretName",
			"        # secretName is the name of the Kubernetes secret that holds the Vault CA certificate.",
			"        # a Kubernetes secret must be in the same namespace that Consul is installed into.",
			"        secretName: \"${20:<kubernetes-secret-name-for-vault-ca-certificate>}\"",
			"        # secretKey",
			"        # secretKey is the key within the Kubernetes secret that holds the Vault CA certificate.",
			"        secretKey: \"${21:<kubernetes-secret-key-for-vault-ca-certificate>}\"",
			"",
			"      # connectCA",
			"      # configuration for the Vault Connect CA provider.",
      "      # the provider will be configured to use the Vault Kubernetes auth method",
      "      # and therefore requires the role provided by 'global.secretsBackend.vault.consulServerRole'",
      "      # to have permissions to the root and intermediate PKI paths.",
      "      # please see https://www.consul.io/docs/connect/ca/vault#vault-acl-policies",
      "      # for information on how to configure the Vault policies.",
			"      connectCA:",
			"        # address",
			"        # the address of the Vault server.",
			"        address: \"${22:<vault-server-address>}\"",
			"",
			"        # authMethodPath",
			"        # the mount path of the Kubernetes auth method in Vault.",
			"        authMethodPath: \"${23:<vault-mount-path-of-k8s-auth-method>}\"",
			"",
			"        # rootPKIPath",
			"        # the path to a PKI secrets engine for the root certificate.",
			"        # please see https://www.consul.io/docs/connect/ca/vault#rootpkipath.",
			"        rootPKIPath: \"${24:<vault-path-of-pki-secrets-engine-for-root-certificate>}\"",
			"",
			"        # intermediatePKIPath",
			"        # the path to a PKI secrets engine for the generated intermediate certificate.",
			"        # please see https://www.consul.io/docs/connect/ca/vault#intermediatepkipath.",
			"        intermediatePKIPath: \"${25:<vault-path-of-pki-secrets-engine-for-intermediate-certificate>}\"",
			"",
			"  # gossipEncryption",
			"  gossipEncryption:",
			"    # autoGenerate",
			"    # automatically generate a gossip encryption key and save it to a Kubernetes secret.",
			"    autoGenerate: ${26|true,false|}",
			"",
			"    # secretName",
			"    # secretName is the name of the Kubernetes secret or Vault secret path that holds the gossip encryption key.",
			"    # a Kubernetes secret must be in the same namespace that Consul is installed into.",
			"    secretName: \"${27:<name-of-k8s-or-vault-secret-path-that-holds-the-gossip-encryption-key>}\"",
			"",
			"    # secretKey",
			"    # secretKey is the key within the Kubernetes secret or Vault secret key that holds the gossip encryption key.",
			"    secretKey: \"${28:<name-of-k8s-or-vault-secret-key-that-holds-the-gossip-encryption-key>}\"",
			"",
			"  # recursors",
			"  # a list of addresses of upstream dns servers that are used to recursively resolve dns queries.",
			"  # these values are given as '-recursor' flags to Consul servers and clients.",
			"  # see https://www.consul.io/docs/agent/options#_recursor for more details.",
			"  # if this is an empty array (the default), then Consul dns will only resolve queries for the Consul top level domain (by default '.consul').",
			"  recursors:",
			"   - \"${29:<upstream-dns-server1>}\"",
			"   - \"${30:<upstream-dns-server2>}\"",
			"   - \"${31:<upstream-dns-serverN>}\"",
			"",
			"  # tls",
			"  # enables tls (https://learn.hashicorp.com/tutorials/consul/tls-encryption-secure)",
			"  # across the cluster to verify authenticity of the Consul servers and clients.",
			"  # requires Consul v1.4.1+.",
			"  tls:",
			"    # enabled",
			"    # if true, the Helm chart will enable tls for Consul servers and clients and all consul-k8s-control-plane components,",
			"    # as well as generate certificate authority (optional) and server and client certificates.",
			"    enabled: ${32|true,false|}",
			"",
			"    # enableAutoEncrypt",
			"    # if true, turns on the auto-encrypt feature on clients and servers.",
			"    # it also switches consul-k8s-control-plane components to retrieve the CA from the server via the API.",
			"    # requires Consul 1.7.1+.",
			"    enableAutoEncrypt: ${33|true,false|}",
			"",
			"    # serverAdditionalDNSSANs",
			"    # a list of additional DNS names to set as Subject Alternative Names (SANs) in the server certificate.",
			"    # this is useful when you need to access the Consul server(s) externally, for example, if you're using the ui.",
			"    serverAdditionalDNSSANs:",
			"      - \"${34:<subject-alternative-name1>}\"",
			"      - \"${35:<subject-alternative-name2>}\"",
			"      - \"${36:<subject-alternative-nameN>}\"",
			"",
			"    # serverAdditionalIPSANs",
			"    # a list of additional ip addresses to set as Subject Alternative Names (SANs) in the server certificate.",
			"    # this is useful when you need to access the Consul server(s) externally, for example, if you're using the ui.",
			"    serverAdditionalIPSANs:",
			"      - \"${37:<subject-alternative-ip1>}\"",
			"      - \"${38:<subject-alternative-ip2>}\"",
			"      - \"${39:<subject-alternative-ipN>}\"",
			"",
			"    # verify",
			"    # if true, 'verify_outgoing', 'verify_server_hostname' and 'verify_incoming_rpc' will be set to 'true' for Consul servers and clients.",
			"    # set this to false to incrementally roll out tls on an existing Consul cluster.",
			"    # please see https://consul.io/docs/k8s/operations/tls-on-existing-cluster for more details.",
			"    verify: ${40|true,false|}",
			"",
			"    # httpsOnly",
			"    # if true, the Helm chart will configure Consul to disable the HTTP port on both clients and servers and to only accept https connections.",
			"    httpsOnly: ${41|true,false|}",
			"",
			"    # caCert",
			"    # a secret containing the certificate of the CA to use for tls communication within the Consul cluster.",
			"    # if you have generated the CA yourself with the Consul cli, you could use the following command to create the secret in Kubernetes:",
			"    #   ```shell-session",
			"    #   $ kubectl create secret generic consul-ca-cert",
			"    #       --from-file='tls.crt=./consul-agent-ca.pem'",
			"    #   ```",
			"    # if you are using Vault as a secrets backend with tls, 'caCert.secretName' must be provided and should reference the CA path for your PKI secrets engine.",
			"    # this should be of the form 'pki/cert/ca' where 'pki' is the mount point of your pki secrets engine.",
			"    # a read policy must be created and associated with the CA cert path for 'global.tls.caCert.secretName'.",
			"    # this will be consumed by the 'global.secretsBackend.vault.consulCARole' role by all Consul components.",
			"    # when using Vault the secretKey is not used.",
			"    caCert:",
			"      # secretName",
			"      # the name of the Kubernetes secret.",
			"      secretName: ${42:\"<kubernetes-secret-name>\"}",
			"",
			"      # secretKey",
			"      # the key of the Kubernetes secret.",
			"      secretKey: ${43:\"<kubernetes-secret-key>\"}",
			"",
			"    # caKey",
			"    # a Kubernetes secret containing the private key of the CA to use for tls communication within the Consul cluster.",
			"    # if you have generated the CA yourself with the Consul cli, you could use the following command to create the secret in Kubernetes:",
			"    #   ```shell-session",
			"    #   $ kubectl create secret generic consul-ca-key",
			"    #       --from-file='tls.key=./consul-agent-ca-key.pem'",
			"    #   ```",
			"    # note that we need the CA key so that we can generate server and client certificates.",
			"    # it is particularly important for the client certificates since they need to have host IPs as Subject Alternative Names.",
			"    # in the future, we may support bringing your own server certificates.",
			"    caKey:",
			"      # secretName",
			"      # the name of the Kubernetes secret.",
			"      secretName: ${44:\"<kubernetes-secret-name>\"}",
			"",
			"      # secretKey",
			"      # the key of the Kubernetes secret.",
			"      secretKey: ${45:\"<kubernetes-secret-key>\"}",
			"",
			"  # acls",
			"  acls:",
			"    # manageSystemACLs",
			"    # if true, the Helm chart will automatically manage ACL tokens and policies for all Consul and consul-k8s-control-plane components.",
			"    # this requires Consul >= 1.4.",
			"    manageSystemACLs: ${46|true,false|}",
			"",
			"    # bootstrapToken",
			"    # a Kubernetes secret containing the bootstrap token to use for creating policies and tokens for all Consul and consul-k8s-control-plane components.",
			"    # if set, we will skip acl bootstrapping of the servers and will only initialize acls for the Consul clients and consul-k8s-control-plane system components.",
			"    bootstrapToken:",
			"      # secretName",
			"      # the name of the Kubernetes secret.",
			"      secretName: ${47:\"<kubernetes-secret-name>\"}",
			"",
			"      # secretKey",
			"      # the key of the Kubernetes secret.",
			"      secretKey: ${48:\"<kubernetes-secret-key>\"}",
			"",
			"    # createReplicationToken",
			"    # if true, an acl token will be created that can be used in secondary datacenters for replication.",
			"    # this should only be set to true in the primary datacenter since the replication token must be created from that datacenter.",
			"    # in secondary datacenters, the secret needs to be imported from the primary datacenter and referenced via 'global.acls.replicationToken'.",
			"    createReplicationToken: ${49|true,false|}",
			"",
			"    # replicationToken",
			"    # replicationToken references a secret containing the replication ACL token.",
			"    # this token will be used by secondary datacenters to perform ACL replication and create ACL tokens and policies.",
			"    # this value is ignored if 'bootstrapToken' is also set.",
			"    replicationToken:",
			"      # secretName",
			"      # the name of the Kubernetes secret or the path of the secret in Vault.",
			"      secretName: ${50:\"<name-of-k8s-secret-or-vault-secret-path>\"}",
			"",
			"      # secretKey",
			"      # the key of the Kubernetes or Vault secret.",
			"      secretKey: ${51:\"<k8s-or-vault-secret-key>\"}",
			"",
			"  # metrics",
			"  metrics:",
			"    # enabled",
			"    # configures the Helm chart’s components to expose Prometheus metrics for the Consul service mesh.",
			"    # by default this includes gateway metrics and sidecar metrics.",
			"    enabled: ${52|true,false|}",
			"",
			"    # enableAgentMetrics",
			"    # configures consul agent metrics. Only applicable if 'global.metrics.enabled' is 'true'.",
			"    enableAgentMetrics: ${53|true,false|}",
			"",
			"    # agentMetricsRetentionTime",
			"    # configures the retention time for metrics in Consul clients and servers.",
